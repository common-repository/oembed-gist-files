{
    "name": "afragen/oembed-gist-files",
    "description": "oEmbed Gist or files within Gists.",
    "type": "wordpress-plugin",
    "keywords": [
        "gist",
        "oembed"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Andy Fragen",
            "email": "andy@thefragens.com",
            "homepage": "https://thefragens.com",
            "role": "Developer"
        },
        {
            "name": "Colin Stewart",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/afragen/oembed-gist-files"
        }
    ],
    "support": {
        "issues": "https://github.com/afragen/oembed-gist-files/issues",
        "source": "https://github.com/afragen/oembed-gist-files"
    },
    "prefer-stable": true,
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "wp-coding-standards/wpcs": "^3.0.0"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "scripts": {
        "wpcs": [
            "vendor/bin/phpcbf .; vendor/bin/phpcs ."
        ]
    }
}
